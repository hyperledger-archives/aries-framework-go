{
  "id": "university_degree",
  "version": "0.1.0",
  "issuer": {
    "id": "did:example:123?linked-domains=3",
    "name": "Example University",
    "styles": {}
  },
  "output_descriptors": [
    {
      "id": "bachelors_degree",
      "schema": "http://example-university.org/1.0.0/bachelors_degree.json",
      "display": {
        "title": {
          "path": [
            "$.title",
            "$.vc.title"
          ],
          "schema": {
            "type": "string"
          },
          "fallback": "Bachelor's Degree'"
        },
        "subtitle": {
          "path": [
            "$.minor",
            "$.vc.minor"
          ],
          "schema": {
            "type": "string"
          },
          "fallback": ""
        },
        "description": {
          "text": "Awarded for completing a four year program at Example University."
        },
        "properties": [
          {
            "path": [
              "$.withDistinction",
              "$.vc.withDistinction"
            ],
            "schema": {
              "type": "boolean"
            },
            "fallback": "Unknown",
            "label": "With distinction"
          },
          {
            "path": [
              "$.yearsStudied",
              "$.vc.yearsStudied"
            ],
            "schema": {
              "type": "number"
            },
            "fallback": "Unknown",
            "label": "Years studied"
          }
        ]
      },
      "styles": {
        "thumbnail": {
          "uri": "http://example-university.org/logo.png",
          "alt": "Example University logo"
        },
        "hero": {
          "uri": "http://example-university.org/hero.png",
          "alt": "Example University students in graduation ceremony"
        },
        "background": {
          "color": "#ff0000"
        },
        "text": {
          "color": "#d4d400"
        }
      }
    }
  ]
}
